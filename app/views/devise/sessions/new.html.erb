<div class ="body-s">

<div class = "login-field">
  <div class = "sub-title">ログインする</div>
  <div class = "sign_in_area">

    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-field">
        <%= f.label :Eメール %><br />
        <%= f.email_field :email, autocomplete: "email", class: "login-form"%>
      </div>

      <div class="form-field">
        <%= f.label :パスワード %><br />
        <%= f.password_field :password, autocomplete: "current-password", class: "login-form" %>
      </div>

      
        <%= f.submit "ログイン", class: "login_submits"%>
        <%= link_to 'もどる', root_path ,class: "login_submits"%>
      
    <% end %>
    
    
  </div>
</div>

</div>